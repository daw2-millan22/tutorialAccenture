
<core:FragmentDefinition
    controllerName="tutorial1.controller.View4"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">
	<VBox id="_IDGenVBox1" class="sapUiSmallMargin">
		<f:Form id="FormChange480_12120"
			editable="true">
			<f:title>
				<core:Title id="_IDGenTitle1" text="Product" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
					labelSpanXL="4"
					labelSpanL="4"
					labelSpanM="12"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="0"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="2"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="_IDGenFormContainer1" title="Product">
					<f:formElements>
						<f:FormElement id="_IDGenFormElement1" label="Name">
							<Text id="_IDGenText12" text="{DetailModel>/ProductName}"/>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement2" label="ProductID">
							<Text id="_IDGenText13" text="{DetailModel>/ProductID}"/>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement3" label="QuantityPerUnit">
							<Text id="_IDGenText1" text="{DetailModel>/QuantityPerUnit}"/>
						</f:FormElement>						
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer id="_IDGenFormContainer2" title="Category">
					<f:formElements>
						<f:FormElement id="_IDGenFormElement5" label="CategoryID">
							<Text id="_IDGenText17" text="{DetailModel>/CategoryID}"/>
						</f:FormElement>
                        <f:FormElement id="_IDFormElement6" label="CategoryName">
								<Text id="_IDGext16" text="{DetailModel>/Category/CategoryName}"/>	
						</f:FormElement>
                        <f:FormElement id="_IDdFormElement6" label="Description">
								<Text id="_IDGdext16" text="{DetailModel>/Category/Description}"/>	
						</f:FormElement>                      
						<f:FormElement id="_IDGenFormElement6" label="Discontinued">
								<Text id="_IDGenText16" text="{DetailModel>/Discontinued}"/>	
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
		<f:Form id="FormChange480_12120-2"
			editable="true">
			<f:title>
				<core:Title id="_IDGenTitle2" text="Supplier" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout2"
					labelSpanXL="4"
					labelSpanL="4"
					labelSpanM="12"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="0"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="2"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="_IDGenFormContainer3" title="Supplier">
					<f:formElements>
						<f:FormElement id="_IDGenFormElement7" label="SupplierID">
							<Text id="qwert" text="{DetailModel>/SupplierID}"/>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement8" label="UnitPrice">
							<Text id="qwewrt" text="{DetailModel>/UnitPrice}"/>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement9" label="UnitsInStock">
							<Text id="qwewqrt" text="{DetailModel>/UnitsInStock}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement10" label="Address">
							<Text id="_IDGenText2" text="{DetailModel>/Supplier/Address}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement11" label="City">
							<Text id="_IDGenText3" text="{DetailModel>/Supplier/City}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement12" label="CompanyName">
							<Text id="_IDGenText4" text="{DetailModel>/Supplier/CompanyName}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement13" label="ContactName">
							<Text id="_IDGenText5" text="{DetailModel>/Supplier/ContactName}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement14" label="Fax">
							<Text id="_IDGenText6" text="{DetailModel>/Supplier/Fax}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement15" label="HomePage">
							<Text id="_IDGenText7" text="{DetailModel>/Supplier/HomePage}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement121" label="Phone">
							<Text id="_IDGenText8" text="{DetailModel>/Supplier/Phone}"/>
						</f:FormElement>
                        <f:FormElement id="_IDGenFormElement122" label="PostalCode">
							<Text id="_IDGenText9" text="{DetailModel>/Supplier/PostalCode}"/>
						</f:FormElement>
                        
                        
                        
                        
					</f:formElements>
                  
				</f:FormContainer>
			</f:formContainers>
            <f:formContainers>
				<f:FormContainer id="_IDGenFormContaigfner3" title="image">
					<f:formElements>
						<f:FormElement id="_IDGenFomElement7" label="image">
							<Image id="_IDGenImage1" src="data:image/bmp;base64,{= ${DetailModel>/Category/Picture}.substr(104)}" width="{/imageWidth}" press="onShowImage"/>
						</f:FormElement>
						
					</f:formElements>
                   
				</f:FormContainer>
			</f:formContainers>
            
		</f:Form>
	</VBox>
    </Page>
</core:FragmentDefinition>
