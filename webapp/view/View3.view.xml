<mvc:View
    controllerName="tutorial1.controller.View3" 
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    
>
  
    <Page id="page" title="{i18n>title}">
        <content>
            <HBox id="_IDGenHBox1" class="myHeaderBarGeneric">
                 <Text  text="CategoryID"></Text>
                <ComboBox class="myComboBoxStyle" id="products-combobox" items="{/Products}"  selectedKey="{ToolModel>/cityName}" >
                    <core:Item id="_IDGenItem1" text="{CategoryID}" key="{CategoryID}"/>
                </ComboBox>
                 <Button id="_IDGenButton2" text="{i18n>NavigateToView} 2" ></Button>
            </HBox>
            <Table id="_IDGenTable1" items="{/Products}" >
                <columns>
                    <Column id="_IDGenColumn1" >
                        <header>
                            <Text id="_IDGenText1" text="PRODUCTO"></Text>
                        </header>
                        <footer />
                    </Column>
                    <Column id="_IDGenColumn2" >
                        <header>
                            <Text id="_IDGenText2" text="PRECIO"></Text>
                        </header>
                        <footer />
                    </Column>
                    <Column id="_IDGenColumn3" >
                        <header>
                            <Text id="_IDGenText3" text="CategoryID"></Text>
                        </header>
                        <footer />
                    </Column>
                    <Column id="_IDGenColumn4" >
                        <header>
                            <Text id="_IDGenText4" text="Discontinued"></Text>
                        </header>
                        <footer />
                    </Column>
                    <Column id="_IDGenColumn5" >
                        <header>
                            <Text id="_IDGenText5" text="PProductID"></Text>
                        </header>
                        <footer />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1" type="Active" press="onPress">
                        <cells>
                            <Text id="_IDGenText6" text="{ProductName}" />
                            <Text id="_IDGenText7" text="{UnitPrice}" />
                            <Text id="_IDGenText8" text="{CategoryID}" />
                            <Text id="_IDGenText9" text="{Discontinued}" />
                            <Text id="_IDGenText10" text="{ProductID}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
         </content>
    </Page>
</mvc:View>